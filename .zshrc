__conda_setup="$('/Users/chrisyoon/miniforge_x86_64/bin/conda' 'shell.zsh' 'hook' 2> /dev/null)"
if [ $? -eq 0 ]; then
    eval "$__conda_setup"
else
    if [ -f "/Users/chrisyoon/miniforge_x86_64/etc/profile.d/conda.sh" ]; then
        . "/Users/chrisyoon/miniforge_x86_64/etc/profile.d/conda.sh"
    else
        export PATH="/Users/chrisyoon/miniforge_x86_64/bin:$PATH"
    fi
fi
